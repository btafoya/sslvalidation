    "type": "library",
    "license": "MIT",
    "keywords": [
        "certificate",
        "ssl",
        "validate",
        "information"
    ],
    "authors": [
        {
            "name": "Brian Tafoya",
            "email": "btafoya@briantafoya.com",
            "homepage": "http://briantafoya.com"
        }
    ],
    "support": {
        "issues": "https://github.com/btafoya/sslvalidation/issues"
    },
    "require": {
        "ext-openssl": "*",
        "ext-pcre": "*",
